<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.benjholla.brainfuck.atlas.feature"
      label="Atlas for Brainfuck"
      version="3.6.0.qualifier"
      provider-name="Ben Holland">

   <description url="https://github.com/benjholla/AtlasBrainfuck">
      Atlas for Brainfuck adds Atlas code mapping support and graph based interpreter support for Brainfuck programs.
   </description>

   <copyright url="https://github.com/benjholla/AtlasBrainfuck">
      Copyright 2018.
   </copyright>

   <license url="https://github.com/benjholla/AtlasBrainfuck">
      All Rights Reserved.
   </license>

   <includes
         id="com.benjholla.brainfuck.eclipse.feature"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="com.ensoftcorp.atlas.core" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="com.ensoftcorp.atlas.ui" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="com.ensoftcorp.atlas.ui.shell" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="com.benjholla.brainfuck.eclipse" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.antlr.v4" version="4.7.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="com.benjholla.brainfuck.atlas"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.antlr.v4"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
